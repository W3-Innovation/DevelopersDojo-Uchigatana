  "tokenColors": [
    {
      "name": "Error: Illegal-Syntax",
      "scope": "invalid.illegal",
      "settings": {
        "foreground": "#FFBBBB",
        "fontStyle": "bold"
      }
    },

    {
      "scope": "entity",
      "settings": {
        "foreground": "#1AB4FF"
      }
    },

    {
      "scope": "variable",
      "settings": {
        "foreground": "#B0B4BE",
        "fontStyle": ""
      }
    },

    {
      "scope": "variable.other.object",
      "settings": {
        "foreground": "#4EB24C",
        "fontStyle": ""
      }
    },

    {
      "scope": "variable.language",
      "settings": {
        "foreground": "#4EB24C",
        "fontStyle": "italic"
      }
    },

    {
      "name": "Parameters",
      "scope": ["variable.parameter", "meta.parameter"],
      "settings": {
        "foreground": "#EEA000",
        "fontStyle": "italic"
      }
    },

    {
      "name": "Properties",
      "scope": "variable.other.property",
      "settings": {
        "foreground": "#B07BFF",
        "fontStyle": ""
      }
    },

    {
      "name": "Operators (Words)",
      "scope": [
        "keyword",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.ternary"
      ],
      "settings": {
        "foreground": "#FF913A",
        "fontStyle": ""
      }
    },

    {
      "name": "Operators (Words) italic",
      "scope": [
        "keyword.control.flow",
        "keyword.control.conditional",
        "keyword.control.trycatch",
        "keyword.control.loop",
        "keyword.control.switch"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },

    {
      "name": "Operators (Special-Characters)",
      "scope": [
        "keyword.operator",
        "punctuation.accessor",
        "storage.type.function.arrow"
